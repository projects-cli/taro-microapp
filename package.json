{
  "name": "miniapp",
  "version": "1.0.0",
  "private": true,
  "description": "野菠萝小程序、h5",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "start:weapp": "npx -p @tarojs/cli@3.1.0-beta.4 npm run dev:weapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "lint": "eslint src --fix",
    "prettier": "prettier src --write",
    "format": "npm run prettier && npm run lint",
    "standard": "standard --fix",
    "commit": "git-cz",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.1.0-beta.4",
    "@tarojs/react": "3.1.0-beta.4",
    "@tarojs/runtime": "3.1.0-beta.4",
    "@tarojs/taro": "3.1.0-beta.4",
    "apollo-link-timeout": "^3.0.0",
    "cross-fetch": "^3.0.6",
    "dva-core": "^2.0.4",
    "dva-loading": "^3.0.22",
    "eslint-config-prettier": "^7.1.0",
    "graphql": "^15.4.0",
    "prettier": "^2.2.1",
    "ramda": "^0.27.1",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "taro-ui": "3.0.0-alpha.7"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@tarojs/mini-runner": "3.1.0-beta.4",
    "@tarojs/webpack-runner": "3.1.0-beta.4",
    "@types/react": "^16.0.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-taro": "3.1.0-beta.4",
    "commitizen": "^4.2.2",
    "commitlint-config-cz": "^0.13.2",
    "conventional-changelog-cli": "^2.1.1",
    "cz-customizable": "^6.3.0",
    "dva": "^2.4.1",
    "eslint": "^7.17.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-jsx": "^10.0.0",
    "eslint-config-taro": "3.1.0-beta.4",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "standard": "^16.0.3",
    "stylelint": "9.3.0",
    "typescript": "^3.7.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "bugs": {
    "url": "https://github.com/yepine/miniapp/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "localStorage"
    ]
  }
}
